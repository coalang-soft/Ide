<xmlStyleHelper>
    <infos>
        <category>SYNTAXMOD</category>
        <pattern>\s*#(defop|literal|snippet)</pattern>
        <styleClass>coalang-syntaxmod</styleClass>
    </infos>
    <infos>
        <category>KEYWORD</category>
        <pattern>\b(if|else|while|for|undefined|null|def|var|return|true|false)\b</pattern>
        <styleClass>coalang-keyword</styleClass>
    </infos>
    <infos>
        <category>VAR</category>
        <pattern>\b(parameters|arguments|eval|integer|float|regex|array|java)\b|@</pattern>
        <styleClass>coalang-var</styleClass>
    </infos>
    <infos>
        <category>PROPERTY</category>
        <pattern>\b(unbind|bind|invoke|properties|add|sub|mul|div|type|native|extend|push|\.\d+)\b</pattern>
        <styleClass>coalang-property</styleClass>
    </infos>
    <infos>
        <category>PRE</category>
        <pattern>\s*#[^\s]*</pattern>
        <styleClass>coalang-pre</styleClass>
    </infos>
    <infos>
        <category>COMMENT</category>
        <pattern>//[^\n]*</pattern>
        <styleClass>coalang-comment</styleClass>
    </infos>
    <infos>
        <category>NUMBER</category>
        <pattern>\d+</pattern>
        <styleClass>coalang-number</styleClass>
    </infos>
    <infos>
        <category>STRING</category>
        <pattern>"([^"\\]|\\.)*"</pattern>
        <styleClass>coalang-string</styleClass>
    </infos>
    <infos>
        <category>SPEC</category>
        <pattern>\[|\]|\(|\)|\{|\}|\.|\,|\;|\/|=|>|&lt;|\+|\-|\:|\|</pattern>
        <styleClass>coalang-spec</styleClass>
    </infos>
</xmlStyleHelper>